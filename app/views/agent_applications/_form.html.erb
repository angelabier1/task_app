<%= simple_form_for(@agent_application) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :license_number %>
    <%= f.input :license_expiration %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
